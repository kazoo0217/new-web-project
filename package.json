{
  "name": "new-web-project",
  "version": "1.0.0",
  "description": "## 環境",
  "main": "main.js",
  "scripts": {
    "start": "run-p js:bundle css:build serve",
    "css:build": "sass --watch src/scss/style.scss:html/css/style.css",
    "css:minify": "sass --style=compressed src/scss/style.scss html/css/style.min.css",
    "js:bundle": "watchify src/js/main.js -o html/js/bundle.js",
    "js:minify": "uglifyjs html/js/bundle.js > html/js/bundle.min.js",
    "serve": "browser-sync start --config bs-config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jquery": "^3.6.0",
    "sanitize.css": "^13.0.0"
  },
  "devDependencies": {
    "browser-sync": "^2.27.5",
    "npm-run-all": "^4.1.5",
    "sass": "^1.42.1",
    "uglify-js": "^3.14.2",
    "watchify": "^4.0.0"
  }
}
